{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/metasearch", "dest": "/api/index.js" },
    { "src": "/generate", "dest": "/api/generate.js" },
    { "src": "/search", "dest": "/api/search.js" },
    { "src": "/analytics", "dest": "/api/analytics.js" },
    { "src": "/health", "dest": "/api/health.js" },
    { "src": "/metasearch", "dest": "/api/metasearch.js" },
    { "src": "/(.*)", "dest": "/api/not-found.js" }
  ]
}
